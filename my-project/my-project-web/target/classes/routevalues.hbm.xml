<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="webapp.dbi.RouteValues" table="routevalues">
    <composite-id>
      <key-property column="id_route" name="id" type="long"/>
      <key-property column="pos" name="pos" type="long"/>
    </composite-id>
    <property name="x">
      <column name="x"/>
    </property>
    <property name="y">
      <column name="y"/>
    </property>
  </class>
</hibernate-mapping>
