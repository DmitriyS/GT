<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
 
<hibernate-mapping>
 <class name="test.group.ui.bean.UserRoutes" table="userroutes" >
  <composite-id>
         <key-property name="uid" type="long" column="id_user"/>
         <key-property name="rid" type="long" column="id_route"/>
  </composite-id>
 </class>
</hibernate-mapping>