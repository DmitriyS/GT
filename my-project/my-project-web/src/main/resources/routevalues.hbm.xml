<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
 
<hibernate-mapping>
 <class name="test.group.dbi.RouteValues" table="ROUTEVALUES" >
  <composite-id>
         <key-property name="id" type="long" column="id_route"/>
         <key-property name="pos" type="long" column="pos"/>
  </composite-id>
  
  <property name="x">
   <column name="x" />
  </property>
  <property name="y">
   <column name="y"/>
  </property>
 </class>
</hibernate-mapping>